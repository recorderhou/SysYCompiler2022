decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()
fun @half(@x : i32) : i32 { 
%entry:
    %x_half_1 = alloc i32
    store @x, %x_half_1
    %0 = load %x_half_1
    %1 = div %0, 2
    ret %1
}
fun @f()  { 
%entry:
    ret 
}
fun @main() : i32 { 
%entry:
    call @f()
    %2 = call @half(10)
    ret %2
}

